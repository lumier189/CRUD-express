            <tr>
                <th>id</th>
                <th>names</th>
                <th>preco</th>
            </tr>
            
        </thead>
        <tbody>
            <% for(var i = 0; i < produtos.length; i++) { %>
            <tr>
                <td>
                    <%= produtos[i].id %>
